# A sample Gemfile
source 'https://rubygems.org'

ruby '2.2.3'

gem 'bundler', require: 'forwardable'
gem 'faraday'
gem 'puma'
gem 'rack'
gem 'redis'

group :development, :test do
  gem 'pry'
end

group :test do
  gem 'minitest'
  gem 'minitest-power_assert'
  gem 'webmock'
  gem 'vcr'
  gem 'coveralls', require: false
end
